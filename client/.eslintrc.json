{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "plugin:react/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "camelcase": "off",
    "react/react-in-jsx-scope": 0,
    "react/jsx-uses-react": 0,
    "react/prop-types": 0,
    "multiline-ternary": "off"
  }
}

